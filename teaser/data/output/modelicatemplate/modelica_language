##get false
<%def name="get_true_false(value)", filter="trim">
  <%
  if value is False:
    return "false"
  elif value is True:
    return "true"
  else:
    return value
  %>
</%def>

##list to modelica list
<%def name="get_list(list)", filter="trim">
  <%
  string_of_list = ""
  for item in list:
    string_of_list += str(item) + ", "
  string_of_list = string_of_list[:-2]
  return string_of_list
  %>
</%def>
